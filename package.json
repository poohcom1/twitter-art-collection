{
  "name": "twitter-art-collection",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "build": "next build",
    "dev": "cross-env PREACT=false next dev",
    "dev:preact": "cross-env PREACT=true next dev",
    "lint": "next lint",
    "type": "tsc",
    "type:watch": "tsc -w",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "jest",
    "test:e2e": "npx playwright test"
  },
  "dependencies": {
